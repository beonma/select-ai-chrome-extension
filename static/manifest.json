{
    "manifest_version": 3,
    "key": "",
    "name": "SelectAI",
    "version": "0.0.2",
    "description": "Instantly rewrite, refine, or expand any selected textâ€”effortlessly using your LLM provider of choice.",
    "action": { "default_popup": "popup.html", "default_title": "SelectAI" },
    "icons": { "128": "icons/icon128.png" },
    "permissions": ["storage"],
    "content_scripts": [{ "js": ["content.bundle.js"], "css": ["content.bundle.css"], "matches": ["<all_urls>"] }],
    "background": { "service_worker": "service-worker.bundle.js" },
    "web_accessible_resources": [{ "matches": ["<all_urls>"], "resources": ["fonts/*.ttf", "fonts/*.woff"] }]
}
